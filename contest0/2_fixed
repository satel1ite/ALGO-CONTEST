#include <cmath>
#include <iomanip>
#include <iostream>
#include <vector>
using namespace std;

int main() {
  int n;
  if (!(cin >> n)) {
    return 0;
  }

  vector<double> a(n);
  for (int i = 0; i < n; ++i) {
    cin >> a[i];
  }

  vector<double> preflogs(n + 1, 0.0);
  if (n >= 1) {
    preflogs[0] = 0.0;
  }
  for (int i = 1; i <= n; ++i) {
    preflogs[i] = preflogs[i - 1] + log(a[i - 1]);
  }

  int q;
  cin >> q;

  const int cPrec = 10;

  for (int i = 0; i < q; ++i) {
    int l;
    int r;
    cin >> l >> r;
    int count = r - l + 1;
    double sumlog;
    sumlog = preflogs[r + 1] - preflogs[l];
    double answer = exp(sumlog / count);
    cout << fixed << setprecision(cPrec) << answer << "\n";
  }
  return 0;
}
